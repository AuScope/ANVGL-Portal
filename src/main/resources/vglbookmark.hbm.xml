<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.auscope.portal.server.vegl.VGLBookMark" table="bookmarks">  
	    <id name="id" column="id">
	            <generator class="native"/>
	    </id>
		<property name="fileIdentifier" not-null="true"/>
		<property name="serviceId" not-null="true"/>			
		<many-to-one name="parent" class="org.auscope.portal.server.web.security.ANVGLUser" column="userId" not-null="true"></many-to-one>	
		
		<bag name="bookMarkDownloads" lazy="false" cascade="all-delete-orphan" inverse="true" >
            <key column="bookmarkId" />
            <one-to-many class="org.auscope.portal.server.vegl.VGLBookMarkDownload" />
        </bag>	 
        
    </class>
</hibernate-mapping>
